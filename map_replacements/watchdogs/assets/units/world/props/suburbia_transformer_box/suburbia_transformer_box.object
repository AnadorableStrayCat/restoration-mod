<dynamic_object>
    <diesel orientation_object="rp_transformer_box" materials="units/world/props/suburbia_transformer_box/suburbia_transformer_box" />
    <sequence_manager file="units/world/props/suburbia_transformer_box/suburbia_transformer_box"/>
	<animations>
        <animation_group name="ag_doors" >
            <object name="a_right_door" />
            <object name="a_left_door" />
        </animation_group>
		<animation_group name="ag_switch" >
            <object name="a_switch" />
		</animation_group>
    </animations>
	<bodies>
		<body name="editor" enabled="true" template="editor" >
			<object name="c_transformer_box" collision_type="mesh_mopp" />
		</body>
		<body name="static_body" enabled="false" template="static" >
			<object name="c_transformer_box" collision_type="mesh_mopp" />
		</body>
		<body name="static_right_door" enabled="false" template="animated" >
			<object name="a_right_door" />
			<object name="c_box_right_door" collision_type="box" padding="-2.5"/>
		</body>
		<body name="static_left_door" enabled="false" template="animated" >
			<object name="a_left_door" />
			<object name="c_box_left_door" collision_type="box" padding="-2.5"/>		
		</body>
	</bodies>
	<decal_surfaces default_material="metal_chassis">
		<decal_mesh name="g_transformer_box" 	enabled="true" 		material="metal_chassis" />
		<decal_mesh name="g_right_door"		 	enabled="true" 		material="metal_chassis" />
		<decal_mesh name="g_left_door" 			enabled="true" 		material="metal_chassis" />
	</decal_surfaces>
    <graphics>
        <object enabled="true" name="g_transformer_box" />
		<object enabled="true" name="s_transformer_box" shadow_caster="true" />
        <object enabled="true" name="g_right_door" />
		<object enabled="true" name="s_right_door" shadow_caster="true" />
        <object enabled="true" name="g_left_door" />
		<object enabled="true" name="s_left_door" shadow_caster="true" />
		<object enabled="true" name="g_transformer_il" />
		<object enabled="true" name="g_switch" />
		
    </graphics>
</dynamic_object>
