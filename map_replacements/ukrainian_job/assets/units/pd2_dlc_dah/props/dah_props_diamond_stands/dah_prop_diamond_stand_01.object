<dynamic_object>
	<diesel materials="units/pd2_dlc_dah/props/dah_props_diamond_stands/dah_prop_diamond_stand_01" orientation_object="rp_dah_prop_diamond_stand_01" />
	<sequence_manager file="units/pd2_dlc_dah/props/dah_props_diamond_stands/dah_prop_diamond_stand_01" />
	
	<animations>
		<animation_group name="anim_cage" enabled="false">
				<object name="a_cage" />
				<object name="a_cylinder" />
		</animation_group>
	</animations>
	
	<bodies>

		<body name="body_static" enabled="true" template="static">
			<object name="rp_dah_prop_diamond_stand_01" />
			<object name="c_box_01" collision_type="box" padding="-2.5" />
		</body>
		<body name="body_anim" enabled="true" template="animated">
			<object name="a_cage" />
			<object name="c_mesh_mopp_top" collision_type="mesh_mopp" padding="-2.5" />
		</body>

		<body name="body_glass_01" enabled="true" template="window" keyframed="true">
			<object name="a_cage" />
			<object name="c_box_glass_01" collision_type="box" />
		</body>
		<body name="body_glass_02" enabled="true" template="window" keyframed="true">
			<object name="a_cage" />
			<object name="c_box_glass_02" collision_type="box" />
		</body>
		<body name="body_glass_03" enabled="true" template="window" keyframed="true">
			<object name="a_cage" />
			<object name="c_box_glass_03" collision_type="box" />
		</body>
		<body name="body_glass_04" enabled="true" template="window" keyframed="true">
			<object name="a_cage" />
			<object name="c_box_glass_04" collision_type="box" />
		</body>

		<body name="body_glass_cut" enabled="false" template="window" keyframed="true">
			<object name="a_cage" />
			<object name="g_glass_cut" collision_type="mesh_mopp" />
		</body>

	</bodies>

	<lights>
		<light name="li_small" enabled="true" editable="true" type="omni|specular" multiplier="0.2" far_range="60" falloff="4" />
	</lights>
	
	<graphics>
		<object name="g_g" enabled="true" />
		<object name="g_glass_01" enabled="true" />
		<object name="g_glass_shattered_01" enabled="false" />
		<object name="g_glass_02" enabled="true" />
		<object name="g_glass_shattered_02" enabled="false" />
		<object name="g_glass_03" enabled="true" />
		<object name="g_glass_shattered_03" enabled="false" />
		<object name="g_glass_04" enabled="true" />
		<object name="g_glass_shattered_04" enabled="false" />
		<object name="g_glass_cut" enabled="false" />
		<object name="g_glass_cut_shattered" enabled="false" />
		<object name="g_upper_part" enabled="true" />
		<object name="g_round_plate" enabled="true" />
		<object name="g_led_lights" enabled="true" />
		<object name="g_lights_glow" enabled="true" />
		<object name="s_s" enabled="true" shadow_caster="true" />
		<object name="s_s_top" enabled="true" shadow_caster="true" />
		<graphic_group name="grp_icon" enabled="true">
			<lod_object name="lod_icon">
				<object name="g_icon_glass_cut" enabled="true"  max_distance="500" />
				<object name="g_icon_glass_cut_lod1" enabled="true" />
			</lod_object>
		</graphic_group>
	</graphics>
</dynamic_object>
